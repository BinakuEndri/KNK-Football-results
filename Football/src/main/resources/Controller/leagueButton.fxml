<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" minHeight="60.0" prefHeight="60.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LeagueButtonController">
    <children>
        <Button fx:id="leagueBtn" minHeight="60.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="287.0" styleClass="leaguebutton" stylesheets="@../css/button.css" text="Home">
            <graphic>
                <ImageView fx:id="leagueImage" fitHeight="45.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/home.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>

</HBox>
